{
    "version":2,
    
  
    "builds":[
        {
            "src":"api/main.py",
            "use":"@vercel/python"
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/build",
            "config":{"distDir":"staticfiles_build"}
        }

    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"api/main.py"
        }
    ]
     
 }
